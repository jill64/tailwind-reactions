{
  "name": "@jill64/tailwind-reactions",
  "version": "0.1.12",
  "description": "UI effect set for TailwindCSS",
  "main": "dist/index.js",
  "license": "MIT",
  "author": "jill64 <intents.turrets0h@icloud.com> (https://github.com/jill64)",
  "bugs": "https://github.com/jill64/tailwind-reactions/issues",
  "homepage": "https://github.com/jill64/tailwind-reactions#readme",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jill64/tailwind-reactions.git"
  },
  "keywords": [
    "tailwindcss",
    "ui",
    "effect"
  ],
  "scripts": {
    "build": "pnpm -r build && npx publint",
    "check": "cd packages/site && pnpm check",
    "prepack": "npm run build",
    "format": "npx prettier --write . --plugin prettier-plugin-svelte .",
    "lint": "npx prettier --check . --plugin prettier-plugin-svelte . && npx eslint"
  },
  "peerDependencies": {
    "tailwindcss": "^3.0.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "6.7.5",
    "@typescript-eslint/parser": "6.7.5",
    "eslint-config-prettier": "9.0.0",
    "eslint-plugin-svelte": "2.34.0",
    "prettier-plugin-svelte": "3.0.3",
    "typescript": "5.2.2"
  },
  "dependencies": {
    "tailwindcss": "3.3.3"
  }
}
